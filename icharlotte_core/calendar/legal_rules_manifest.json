      "name": "MSJ Notice of Motion",
      "statute": "CCP § 437c(a)(2)",
      "description": "Motion for summary judgment must be served 75 calendar days before hearing",
      "logic": {
        "offset": 75,
        "unit": "calendar_days",
        "direction": "backward"
      },
      "service_extensions": {
        "electronic": {"days": 2, "unit": "court_days"},
        "mail_ca": {"days": 5, "unit": "calendar_days"},
